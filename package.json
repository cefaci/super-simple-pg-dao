{
  "name": "super-simple-pg-dao",
  "description": "Super Simple PG DAO",
  "version": "0.2.0",
  "main": "app.js",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/super-simple-pg-dao.git"
  },
  "scripts": {
    "clean": "rm -R dist/*",
    "build": "npm run clean && babel src --out-dir dist --source-maps",
    "serv": "node dist/app.js",
    "start": "nodemon --experimental-modules --watch src/app --exec babel-node src/app.js --trace-warnings --experimental-modules --watch src --env=development",
    "test": "mocha --opts ./test/mocha.opts",
    "prepush": "npm run lint",
    "lint": "eslint --config .eslintrc src test --cache",
    "lint:fix": "eslint --config .eslintrc src test --fix"
  },
  "dependencies": {
    "pg-promise": "^10.5.8",
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "mocha": "^8.0.1",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "eslintIgnore": [
    "/dist/*",
    "/node_modules/*",
    "/log/*"
  ],
  "modulePaths": [
    "<rootDir>/src",
    "<rootDir>/node_modules"
  ],
  "contributors": [
    {
      "name": "cefaci",
      "email": "25903524+cefaci@users.noreply.github.com",
      "url": "https://github.com/super-simple-pg-dao"
    }
  ]
}
